<mah:MetroWindow x:Class="Astrolo.Explorer.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:local="clr-namespace:Astrolo.Explorer.UI"
        mc:Ignorable="d"
        Height="450" Width="800"
        d:DataContext="{d:DesignInstance {x:Type local:MainViewModel}, IsDesignTimeCreatable=False}">
    <TabControl x:Name="Items" TabStripPlacement="Bottom" IsTabStop="False" Focusable="False" />
</mah:MetroWindow>
